{
    "name": "ello-web",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "check-types": "tsc --project tsconfig.json --pretty --noEmit",
        "test": "vitest",
        "test:e2e": "pnpm exec playwright test",
        "run-mock-server": "vite-node mock-server.ts | pino-pretty -c",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "codegen:api": "orval --config orval.config.ts && node src/api/internal/postprocess-orval.mjs && eslint --fix \"src/api/models/**/*.ts\" \"src/api/schemas/**/*.ts\" && prettier --write \"src/api/models/**/*.ts\" \"src/api/schemas/**/*.ts\"",
        "workflow:i18n": "tsx scripts/i18nWorkflow/index.ts",
        "i18n": "pnpm run workflow:i18n && prettier --write \"locales/**\"",
        "prepare": "uv run pre-commit install --install-hooks || true"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.1",
        "@radix-ui/react-dialog": "^1.1.4",
        "@radix-ui/react-dropdown-menu": "^2.1.4",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-select": "^2.1.4",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-switch": "^1.1.2",
        "@tanstack/react-query": "^5.62.8",
        "@tanstack/react-query-devtools": "^5.62.8",
        "axios": "^1.7.9",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "i18next": "^25.8.13",
        "i18next-browser-languagedetector": "^8.2.1",
        "i18next-resources-to-backend": "^1.2.1",
        "lucide-react": "^0.562.0",
        "nanoid": "^5.0.9",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-error-boundary": "^4.1.2",
        "react-helmet-async": "^2.0.5",
        "react-hook-form": "^7.54.2",
        "react-i18next": "^16.5.4",
        "react-query-auth": "^2.4.3",
        "react-router": "^7.1.1",
        "react-router-dom": "^7.1.1",
        "tailwind-merge": "^2.6.0",
        "zod": "^3.24.1",
        "zustand": "^5.0.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@mswjs/data": "^0.16.2",
        "@mswjs/http-middleware": "^0.10.1",
        "@ngneat/falso": "^8.0.2",
        "@playwright/test": "^1.58.2",
        "@storybook/addon-a11y": "^8.6.17",
        "@storybook/addon-essentials": "^8.6.14",
        "@storybook/addon-interactions": "^8.6.14",
        "@storybook/addon-links": "^8.6.17",
        "@storybook/node-logger": "^8.6.14",
        "@storybook/react": "^8.6.17",
        "@storybook/react-vite": "^8.6.17",
        "@tailwindcss/typography": "^0.5.15",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.5",
        "@types/react": "^19.0.2",
        "@types/react-dom": "^19.0.2",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "^2.1.8",
        "@vitest/ui": "^2.1.8",
        "autoprefixer": "^10.4.20",
        "cors": "^2.8.5",
        "eslint": "^9.17.0",
        "eslint-plugin-check-file": "^2.8.0",
        "eslint-plugin-import-x": "^4.6.1",
        "eslint-plugin-playwright": "^2.8.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "express": "^4.21.2",
        "globals": "^15.14.0",
        "jsdom": "^25.0.1",
        "msw": "^2.12.10",
        "orval": "^8.4.2",
        "pino-pretty": "^13.1.3",
        "pm2": "^6.0.14",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "storybook": "^8.6.17",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "tsx": "^4.21.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.19.0",
        "vite": "^6.0.7",
        "vite-node": "^5.3.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^2.1.8"
    },
    "packageManager": "pnpm@10.29.3"
}